<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <setting name="rwf.session.allowLongTimeLogin" value="true" type="bool"/>     <!-- Langzeit Login erlauben -->
    <setting name="rwf.date.Timezone" value="Europe%2FBerlin" type="string"/>     <!-- Zeitzone -->
    <setting name="rwf.date.defaultDateFormat" value="d.m.Y" type="string"/>      <!-- Standard Datums Format -->
    <setting name="rwf.date.defaultTimeFormat" value="H%3Ai%3As" type="string"/>  <!-- Standard Zeit Format -->
    <setting name="rwf.date.useTimeline" value="true" type="bool"/>               <!-- Zeitschienen Funktion verwenden -->
    <setting name="rwf.date.sunriseOffset" value="0" type="int"/>                 <!-- Zeitverschiebung Sonnenaufgang -->
    <setting name="rwf.date.sunsetOffset" value="0" type="int"/>                  <!-- Zeitverschiebung Sonnenuntergang -->
    <setting name="rwf.date.Latitude" value="50.00" type="float"/>                <!-- Laengengrad fuer Berechnung Sonnenauf/untergang -->
    <setting name="rwf.date.Longitude" value="12.00" type="float"/>               <!-- Breitengrad fuer Berechnung Sonnenauf/untergang -->
    <setting name="rwf.language.defaultLanguage" value="de" type="string"/>       <!-- Standardsprache -->
    <setting name="rwf.fritzBox.address" value="fritz.box" type="string"/>        <!-- FritzBox Adresse (Host oder IP) -->
    <setting name="rwf.fritzBox.has5GHzWlan" value="false" type="bool"/>          <!-- hat die FritzBox ein 5GHz WLan -->
    <setting name="rwf.fritzBox.user" value="" type="string"/>                    <!-- FritzBox Benutzer -->
    <setting name="rwf.fritzBox.password" value="" type="string"/>                <!-- FritzBox Passwort -->
</settings>
